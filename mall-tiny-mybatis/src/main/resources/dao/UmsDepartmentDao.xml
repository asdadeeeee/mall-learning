<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.macro.mall.tiny.dao.UmsDepartmentDao">
    <select id="SelectSingleByID" resultType="com.macro.mall.tiny.model.UmsDepartment">
        select * from ums_department where id = #{id}
    </select>

    <insert id="InsertSingle">
        insert into ums_department(name,number)
        values (#{name}, #{number})
        <selectKey keyColumn="id" resultType="long" keyProperty="id" order="AFTER">
            SELECT LAST_INSERT_ID()
        </selectKey>
    </insert>

</mapper>